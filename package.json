    "reflect-metadata": "^0.1.13",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@types/cookie-session": "^2.0.37",
    "@types/dotenv": "^6.1.1",
    "@types/mongoose": "^5.5.12",
